{
    "name": "mehedimi/laravel-sendgrid",
    "version": "0.0.1",
    "description": "A package for supporting sendgrid as an email sending driver",
    "type": "package",
    "license": "MIT",
    "keywords": ["laravel-mail", "laravel-sendgrid", "sendgrid mailable"],
    "authors": [
        {
            "name": "Mehedi Hasan",
            "email": "mehedihasansabbirmi@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mehedi\\Sendgrid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mehedi\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/mail": "^7|^8",
        "guzzlehttp/guzzle": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "illuminate/config": "^8.60",
        "symfony/var-dumper": "^5.3",
        "mockery/mockery": "^1.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mehedi\\Sendgrid\\LaravelSendgridServiceProvider"
            ]
        }
    }
}
